package university;

public class Teacher {
	private String teacherName;
	private String teacherSurn;
	private boolean ifRector = false;
	public String getTeacherName() {
		return teacherName;
	}
	public void setTeacherName(String teacherName) {
		this.teacherName = teacherName;
	}
	public String getTeacherSurn() {
		return teacherSurn;
	}
	public void setTeacherSurn(String teacherSurn) {
		this.teacherSurn = teacherSurn;
	}
	public boolean isIfRector() {
		return ifRector;
	}
	public void setIfRector(boolean ifRector) {
		this.ifRector = ifRector;
	}
}
